[user]
    name = Randall Hammond
    email = {{ .email }}
[core]
    autocrlf = input
    safecrlf = true
    editor = vim
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
  type = cat-file -t
  dump = cat-file -p
